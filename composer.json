{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "devfactory/minify": "1.0.7",
        "fideloper/proxy": "3.3.4",
        "laravel/framework": "v5.5.40",
        "laravel/tinker": "v1.0.7",
        "mews/purifier": "2.1.0",
        "spatie/laravel-activitylog": "2.7.0",
        "lord/laroute": "2.4.7",
        "axn/laravel-laroute": "1.3.0",
        "html2text/html2text": "4.1.0",
        "webklex/laravel-imap": "1.2.7",
        "watson/rememberable": "2.0.4",
        "nwidart/laravel-modules": "2.7.0",
        "tormjens/eventy": "0.5.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "v2.4.3",
        "filp/whoops": "2.2.0",
        "fzaninotto/faker": "v1.7.1",
        "mockery/mockery": "1.1.0",
        "phpunit/phpunit": "6.5.8"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
